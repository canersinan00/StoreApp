@model IEnumerable<CategoryViewModel>
@using StoreApp.Web.Models

<div class="d-grid gap-2">
    <a class="btn btn-outline-secondary" asp-action="Index" asp-controller="Home">Home Page</a>

    @foreach (var category in Model)
    {
        <a class="btn btn-outline-secondary" asp-action="Index" asp-controller="Home">@category.Name</a>
    }
</div>